<tr>
  {{#each columns as |column|}}
    {{#if column.isActive}}
      {{#if (compare-with columnsConfig "property" column.property "isCustomTemplate")}}
        {{yield (hash
          cell=(component "ember-tabular-cell" column=column record=row colProperty=colProperty)
        )}}
      {{else}}
        {{ember-tabular-cell column=column record=row colProperty=column.property}}
      {{/if}}
    {{/if}}
  {{/each}}
</tr>
